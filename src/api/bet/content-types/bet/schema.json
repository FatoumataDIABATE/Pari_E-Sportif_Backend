{
  "kind": "collectionType",
  "collectionName": "bets",
  "info": {
    "singularName": "bet",
    "pluralName": "bets",
    "displayName": "Bet"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "amount": {
      "type": "decimal"
    },
    "placed_at": {
      "type": "datetime"
    },
    "etat": {
      "type": "enumeration",
      "enum": [
        "pending",
        "won",
        "lost"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "match": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::match.match",
      "inversedBy": "bets"
    },
    "pronostic": {
      "type": "enumeration",
      "enum": [
        "A",
        "B",
        "N"
      ]
    },
    "amount_final": {
      "type": "decimal"
    }
  }
}
